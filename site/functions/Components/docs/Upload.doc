/**
 * @title 文件上传
 */
import { Component } from 'react'
import Upload from 'bfd/Upload'

class UploadFile extends Component {

  render() {
    const props = {
      method: 'post',
      action: '/upload.do',
      onComplete: ::this.handleComplete,
      multiple: true,
      showFileList: true
    }
    return (
      <div>
        <Upload {...props} />
      </div>
    )
  }

  handleComplete(data, list) {
    console.log('complete:', data, 'list:', list)
  }
}

@component Upload