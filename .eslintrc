{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "blockBindings": true,
    "modules": true,
    "jsx": true
  },
  "rules": {
    "no-unused-vars": [1],
    "indent": [
      2,
      2
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "never"
    ]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}