{
  "plugins": [
    "react"
  ],
  "ecmaFeatures": {
    "blockBindings": true,
    "modules": true,
    "experimentalObjectRestSpread": true,
    "jsx": true
  },
  "rules": {
    "no-var": 2,
    "jsx-quotes": 2,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/wrap-multilines": 2,
    "react/self-closing-comp": 2,
    "indent": [2, 2],
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "never"]
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}